<form method="POST">
  <label for="first_name">First Name:</label>
  <input type="text" name="first_name">
  <label for="last_name">Last Name:</label>
  <input type="text" name="last_name">
  <button type="submit">Submit</button>
</form>


<ul>
  {{#people}}
  <li>{{first_name}} - {{last_name}}</li>
  {{/people}}
</ul>